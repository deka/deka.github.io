<div class="item-header">
    {{#if prepend}}
    <div class="item-header-prepend">
        {{prepend}}
    </div>
    {{/if}}
    {{#if title}}
    <div class="item-header-title">
        {{title}}
    </div>
    {{/if}}
    {{#if subtitle}}
    <div class="item-header-subtitle">
        {{subtitle}}
    </div>
    {{/if}}
</div>
