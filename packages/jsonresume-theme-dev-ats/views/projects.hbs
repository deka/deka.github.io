{{#if resume.projects.length}}
<section id="projects">
  <h2>Projects</h2>
  <div class="item">
    {{#each resume.projects}}
      {{#if name}}{{#if description}}
        
        {{#if url}}
          {{#> item
            title=name
            subtitle=description
            url=url
            mainUrl='y'
          }}
          {{/item}}
        {{else}}
          {{#> item
            title=name
            subtitle=description
            url=''
            mainUrl=''
          }}
          {{/item}}
        {{/if}}{{/if}}
        
      {{/if}}
    {{/each}}
  </div>
</section>
{{/if}}
